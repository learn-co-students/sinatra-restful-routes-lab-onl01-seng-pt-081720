<!DOCTYPE html>
  <html>
    <body>
      <h1>Recipes</h1>
        <% @recipes.each do |recipe| %>
          <strong><a href="/recipes/<%=recipe.id %>"><%= recipe.name %></a><strong><br>
          <p><%= recipe.ingredients %></p><br>
          <p><%= recipe.cook_time %></p><br>

          <br>
          <% end %>
        </body>
      </html>
